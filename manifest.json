{
  "manifest_version": 3,
  "name": "ChatGPT Manager",
  "version": "1.2.4",
  "description": "Organisez vos chats IA dans des dossiers avec drag & drop",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
